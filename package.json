{
	"name": "vibemap-blocks",
	"version": "0.1.1",
	"description": "Draft blocks for Vibemap listings, events and maps scaffolded with Create Block tool.",
	"author": "Vibemap, Inc.",
	"license": "GPL-2.0-or-later",
	"main": "build/index.js",
	"files": [
		"build/**/*",
		"src/**/*",
		"*.php",
		"*.css",
		"*.md",
		"*.txt"
	],
	"scripts": {
		"build": "wp-scripts build",
		"build-folders": "wp-scripts build --webpack-copy-php --webpack-src-dir=src/blocks --output-path=build/blocks",
		"watch-folders": "wp-scripts start --webpack-copy-php --webpack-src-dir=src/blocks --output-path=build/blocks",
		"check-engines": "wp-scripts check-engines",
		"check-licenses": "wp-scripts check-licenses",
		"format": "wp-scripts format",
		"lint:css": "wp-scripts lint-style",
		"lint:js": "wp-scripts lint-js",
		"packages-update": "wp-scripts packages-update",
		"plugin-zip": "wp-scripts plugin-zip",
		"start": "wp-scripts start",
		"test:e2e": "wp-scripts test-e2e",
		"test:unit": "wp-scripts test-unit-js"
	},
	"devDependencies": {
		"@wordpress/scripts": "^30.0.2"
	},
	"dependencies": {
		"vibemap-constants": "^1.7.16"
	}
}
